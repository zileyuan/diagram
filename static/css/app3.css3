/* Clean up the lists styles */
ul.accordion {
    list-style: none;
    margin: 0;
    padding: 0;
}

/* Hide the radio buttons */
/* These are what allow us to toggle content panes */
ul.accordion input[type='checkbox'] {
    display: none;
}

ul.accordion input[type='radio'] {
    display: none;
}

/* Give each content pane some styles */
ul.accordion li {
    /*background-color: #F9F9F9;*/
    /*border-bottom: 1px solid #e2e2e2;*/
    /*padding-bottom: 20px;*/
}

/* Make the main tab look more clickable */
ul.accordion label {
    /*background-color: #FDFEFF;*/
    color: #363739;
    display: block;
    padding: 3px;
    font-family: "微软雅黑", "FontAwesome" !important;
}

ul.accordion label:before {
    font-size: 17px;
    padding-right: 5px;
    color: #A6B0B9;
    font-family: "微软雅黑", "FontAwesome", "lcwebfont" !important;
}

ul.accordion label:hover {
    cursor: pointer;
}

/* Set up the div that will show and hide */
ul.accordion div.content {
    overflow: hidden;
    padding: 0px 0px 15px 0px;
    display: none;
    /*border-bottom: 1px solid #e2e2e2;*/
}

ul.accordion ul.content {
    overflow: hidden;
    padding: 0px;
    display: none;
}

/* Show the content boxes when the radio buttons are checked */
ul.accordion input[type='checkbox']:checked + label + div.content {
    display: block;
}

ul.accordion input[type='radio']:checked + label + ul.content {
    display: block;
}

